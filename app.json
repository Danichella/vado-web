{
  "expo": {
    "name": "Vado",
    "slug": "vado_web",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vado",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#0f172a",
    "splash": {
      "image": "./assets/images/vado_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vado.web"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vado_logo.png",
        "backgroundColor": "#0f172a"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ],
      "package": "com.vado.web"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/vado_logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Vado virtual assistant to use your location.",
          "isAndroidForegroundServiceEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b42d485d-1de6-43aa-937e-bcb645e6ed2a"
      }
    }
  }
}
